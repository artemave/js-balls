{
  "name": "vjs",
  "version": "0.0.1",
  "scripts": {
    "test": "mocha *spec.js && ./run_tests",
    "build": "yarn build:osx && yarn build:linux",
    "build:osx": "pkg --target macos --out-path dist js_language_server.js",
    "build:linux": "noderify --filter fsevents --filter spawn-sync js_language_server.js > dist/js_language_server.js"
  },
  "dependencies": {
    "@babel/parser": "^7.8.7",
    "@babel/traverse": "^7.8.6",
    "js-editor-tags": "^0.1.3",
    "yargs": "^15.3.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "mocha": "^7.1.0",
    "noderify": "^5.0.0",
    "pkg": "^4.4.4"
  }
}
